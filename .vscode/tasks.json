{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build Rust CLI App",
			"command": "cargo build",
			"group": "build",
			"problemMatcher": [
				"$rustc"
			]
		},
		{
			"type": "shell",
			"label": "Run Rust CLI App",
			"command": "cargo run",
			"group": "test",
			"problemMatcher": [
				"$rustc"
			]
		}
	]
}